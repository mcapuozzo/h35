<?xml version = "1.0"?>
<xs:schema xmlns:xs=""
targetNamespace=""
xmlns=""
elementFormDefault="">

<xs:element name = "CFG">
  <xs:complexType>
    <xs:sequence>
      <xs:element name = "CFG-Line1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name = "Station-Name" type = "xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:minLength min-value = "1"/>
                  <xs:maxLength max-value= "64"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name = "Device-ID" type = "xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:String">
                  <xs:minLength min-value= "1"/>
                  <xs:maxLength max-value= "64"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name= "Revision-Year" type = "xs:integer">
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:enumeration value="1991"/>
                  <xs:enumeration value="1999"/>
                  <xs:enumeration value="2013"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name = "CFG-Line2">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Total-Channels" type = "xs:integer">
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:minInclusive value="1"/>
                  <xs:MaxInclusive value="999999"/>
                </xs:restriction>
              </xs.simpleType>
            </xs:element>
            <xs:element name="Analog-Channels" type="xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:String">
                  <xs:minLength min-value = "2"/>
                  <xs:maxLength max-value= "7"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="Dialog-Channels" type="xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:String">
                  <xs:minLength min-value = "2"/>
                  <xs:maxLength max-value = "7"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Analog-Channels">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="A-Channel-Number" type="xs:integer">
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:minInclusive value="1"/>
                  <xs:maxInclusive value="999999"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="A-Channel-ID" type="xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:String">
                  <xs:minLength min-value= "1"/>
                  <xs:maxLength max-value= "128"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="A-Ph" type="xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:String>
                  <xs:minLength min-value= "0"/>
                  <xs:maxLength max-value= "2"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="A-CCBM" type="xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:String>
                  <xs:minLength min-value= "0"/>
                  <xs:maxLength max-value= "64"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="UU" type="xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:String">
                  <xs:minLength min-value= "1"/>
                  <xs:maxLength max-value= "32"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="A" type="xs:decimal">
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:pattern value="\d{1,32}"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="B" type="xs:decimal">
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:pattern value="\d{1,32}"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="Skew" type="xs:decimal">
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:pattern value="\d{1,32}"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="Min" type="xs:decimal">
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:pattern value="\d{1,13}"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="Max" type="xs:decimal">
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:pattern value="\d{1,13}"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="Primary" type="xs:decimal">
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:pattern value="\d{1,32}"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="Secondary" type="xs:decimal">
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:pattern value="\d{1,32}"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="Primary-or-Secondary" type="xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:String">
                  <xs:enumerated value="P"/>
                  <xs:enumerated value="S"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Digital-Channels">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="D-Channel-Number" type="xs:integer">
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:minInclusive value="1"/>
                  <xs:maxInclusive value="999999"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="D-Channel-ID" type="xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:String"/>
                  <xs:minLength = "1"/>
                  <xs:maxLength = "128"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="D-Ph" type="xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:String"/>
                  <xs:minLength = "0"/>
                  <xs:maxlength = "2"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="D-CCBM" type="xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:String"/>
                  <xs:minLength = "0"/>
                  <xs:maxLength = "64"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="Y" type="xs:integer">
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:minInclusive value = "0"/>
                  <xs:maxInclusive value = "1"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Line-Frequency">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="LF" type="xs:decimal">
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:pattern value="\d{1,32}"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Sampling-Rate-Info">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="NRates" type="xs:integer">
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:minInclusive value="0"
                  <xs:maxInclusive value="999"
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="Samp" type="xs:decimal">
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:pattern value="\d{1,32}"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="End-Samp" type="xs:integer">
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:minInclusive value="1"/>
                  <xs:maxInclusive value="9999999999"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Date-and-Time">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="dd" type="xs:integer">
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:minInclusive value="1"/>
                  <xs:maxInclusive value="31"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="mm" type="xs:integer">
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:minInclusive value="1"/>
                  <xs:maxInclusive value="12"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="yyyy" type="xs:integer">
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:minInclusive value="1900"/>
                  <xs:maxInclusive value="9999"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="hh" type="xs:integer">
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:minInclusive value="0"/>
                  <xs:maxInclusive value="23"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="mm" type="xs:integer">
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:minInclusive value="0"/>
                  <xs:maxInclusive value="59"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="ss.ssssss" type="xs:decimal">
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:minInclusive value="00.000000"/>
                  <xs:maxInclusive value="59.999999"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="DAT-File-Type">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="FT" type="xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:String">
                  <xs:minLength = "5"/>
                  <xs:maxLength = "8"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Time-Stamp-MF">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Time-Mult" type="xs:decimal">
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:pattern value="\d{1,32}"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Time-Information-and-Relationship">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Time-Code" type="xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:String">
                  <xs:minLength = "1"/>
                  <xs:maxLength = "6"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="Local-Code" type="xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:String">
                  <xs:minLength = "1"/>
                  <xs:maxLength = "6"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Time-Quality-of-Samps">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Tmq-Code" type="xs:String">
              <xs:simpleType>
                <xs:restriction base="xs:String">
                  <xs:maxLength = "1"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>
 
              
                


























      
    
